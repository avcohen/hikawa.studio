{
  "name": "kame-app",
  "version": "1.0.0",
  "description": "Frontend app for kame-case.com",
  "main": "index.js",
  "author": "Joseph Thomas / Good Idea Studio",
  "license": "MIT",
  "scripts": {
    "dev:app": "cross-env ENV=development DEBUG=api webpack-dev-server --colors --env development --config ./webpack/webpack.config.dev.js",
    "dev:server": "cross-env ENV=development cross-env DEBUG=server nodemon --watch ./src/server ./src/server/server.js --exec babel-node",
    "dev:tunnel": "cross-env SHARE_TUNNEL=true yarn dev",
    "clean": "rimraf node_modules/.cache && rimraf build && rimraf public/js",
    "build": "yarn clean && yarn build:app && yarn build:server",
    "build:server": "NODE_ENV=production yarn babel --copy-files --include-dotfiles src -d build",
    "build:app": "NODE_ENV=production webpack -p --env production --progress --colors --config ./webpack/webpack.config.js",
    "stats:app": "cross-env WEBPACK_STATS=true cross-env NODE_ENV=production webpack -p --env production --config ./webpack/webpack.config.js --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "deploy": "yarn build && now && now alias",
    "start": "NODE_ENV=production node ./build/server/server.js"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
