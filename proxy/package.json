{
  "name": "kame-proxy",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Joseph Thomas | Good Idea Studio",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "cross-env DEBUG=server nodemon --delay 1250ms --watch ./src --watch $(realpath ./node_modules/@good-idea/shopify-sanity-proxy-server) ./src/server.js",
    "bundle": "babel ./src --out-dir ./dist",
	"deploy": "yarn bundle && now && now alias"
  },
  "dependencies": {
    "@good-idea/shopify-sanity-proxy-server": "^0.0.6",
    "dotenv": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.7"
  }
}
