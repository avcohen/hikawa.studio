    "@sanity/core": "^1.149.18",
    "@sanity/default-layout": "^1.149.18",
    "@sanity/default-login": "^1.149.18",
    "@sanity/desk-tool": "^1.149.18",
    "@sanity/vision": "^1.149.16",
    "prop-types": "^15.7",
    "react": "^16.13",
    "react-dom": "^16.13",
    "react-icons": "^3.10.0",
    "sanity-plugin-media": "^0.2.13"
